	<content><![CDATA[
// Initialiase variables.
\$db = JFactory::getDbo();
\$query = \$db->getQuery(true);

// Prepare query.
\$query->select('');
\$query->from('#__');
\$query->where('');

// Inject the query and load the result.
\$db->setQuery(\$query);
\$result = \$db->loadObjectList();

// Check for errors.
if (\$error = \$db->getErrorMsg())
{
	\$this->setError($error);

	return false;
}

]]></content>
